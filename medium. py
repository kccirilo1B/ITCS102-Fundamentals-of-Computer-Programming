col = eval(input("Enter number of columns: "))

for i in range(1, 11, 1):
    for kc in range(1, col+1, 1):
        print(kc, "*", i, "=", kc*i, end="\t")
    print()

for m in range(1, 6, 1):
    for k in range(6, m, -1):
        print(" ", end=" ")
    for c in range(1, m+2, 1):
        print("* ", end=" ")
    print()
